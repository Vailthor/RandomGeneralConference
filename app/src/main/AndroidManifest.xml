<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.personal.nathan.randomgeneralconferencetalk">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:minSdkVersion="17"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="com.personal.nathan.randomgeneralconferencetalk.History"
            android:parentActivityName="com.personal.nathan.randomgeneralconferencetalk.MainActivity" />
        <activity
            android:name="com.personal.nathan.randomgeneralconferencetalk.TalkView"
            android:parentActivityName="com.personal.nathan.randomgeneralconferencetalk.MainActivity" />
        <activity
            android:name="com.personal.nathan.randomgeneralconferencetalk.Settings"
            android:parentActivityName="com.personal.nathan.randomgeneralconferencetalk.MainActivity" />
        <activity
            android:name="com.personal.nathan.randomgeneralconferencetalk.MainActivity"
            android:label="@string/app_name"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:exported="true"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>